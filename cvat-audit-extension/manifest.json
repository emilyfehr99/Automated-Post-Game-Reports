{
  "manifest_version": 3,
  "name": "CVAT Audit Logger",
  "version": "0.1.0",
  "description": "Quickly log annotation audit issues in CVAT with timestamps and export options.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "CVAT Audit Logger"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}

